<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="10.0.3" />
  </ItemGroup>

  <!-- Exclude repo-level build and test artifacts from this project -->
  <ItemGroup>
    <Compile Remove="bin\**" />
    <Compile Remove="obj\**" />
    <Compile Remove="tests\**" />
    <EmbeddedResource Remove="bin\**;obj\**;tests\**" />
    <None Remove="bin\**;obj\**;tests\**" />
  </ItemGroup>

</Project>
